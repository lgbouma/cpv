@article{douglas_poking_2017,
	title = {Poking the {Beehive} from {Space}: {K2} {Rotation} {Periods} for {Praesepe}},
	volume = {842},
	issn = {0004-637X},
	shorttitle = {Poking the {Beehive} from {Space}},
